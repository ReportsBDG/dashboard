# Google Apps Script Configuration
# Replace with your actual Google Apps Script deployment URL
NEXT_PUBLIC_GOOGLE_APPS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Dashboard Configuration
NEXT_PUBLIC_APP_NAME="Dental Analytics Dashboard"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# API Configuration
NEXT_PUBLIC_API_TIMEOUT=30000
NEXT_PUBLIC_CACHE_DURATION=300000

# Feature Flags
NEXT_PUBLIC_ENABLE_DARK_MODE=true
NEXT_PUBLIC_ENABLE_EXPORTS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
